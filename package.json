    "nvm": "0.0.4",
    "resolve-url-loader": "^2.3.2",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.8"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
